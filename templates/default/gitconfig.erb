[core]
  editor = vim -f
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
[color]
  branch = auto
  diff = auto
  grep = auto
  interactive = auto
  status = auto
  ui = 1
[user]
  name = Change Me
  email = change@me
[branch]
  autosetuprebase = always
